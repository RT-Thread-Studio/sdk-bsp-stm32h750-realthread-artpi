from building import *

cwd = GetCurrentDir()

if GetDepend('RT_USING_LWIP202') or GetDepend('RT_USING_LWIP210') :
    src = ['dhcp_server_raw.c']
else:
    src = ['dhcp_server.c']

CPPPATH = [cwd]

group = DefineGroup('lwIP', src, depend = ['RT_USING_LWIP', 'LWIP_USING_DHCPD'], CPPPATH = CPPPATH)

Return('group')
